<config>
    <modules>
        <CosmoCommerce_Sales>
            <version>1.0.0</version>
        </CosmoCommerce_Sales>
    </modules>
    
    
    
    
    <global> 
        <blocks>
            <sales>
                <rewrite>
                    <order_history>CosmoCommerce_Sales_Block_Order_History</order_history>
                </rewrite>
            </sales>
        </blocks>
        <models>
            <sales>
                <rewrite>
                    <order>CosmoCommerce_Sales_Model_Order</order>
                </rewrite>
            </sales>
        </models>
    </global>
    <frontend>
        <translate>
            <modules>
                <CosmoCommerce_Sales>
                    <files>
                        <default>Mage_Sales.csv</default>
                    </files>
                </CosmoCommerce_Sales>
            </modules>
        </translate>
        <routers>
            <sales>
                <args>
                    <modules>
                        <CosmoCommerce_Sales before="Mage_Sales">CosmoCommerce_Sales</CosmoCommerce_Sales>
                    </modules>
                </args>
            </sales>
        </routers>
    </frontend>
</config>
